{
  "name": "ssswang/qualtrics-api-php",
  "version": "1.0.0",
  "type": "library",
  "description": "PHP library for v3 of the Qualtrics API",
  "keywords": ["qualtrics", "survey"],
  "homepage": "https://github.com/UI-Research/qualtrics-api-php",
  "require": {
    "php": ">=5.6.0",
    "guzzlehttp/guzzle": "^6.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8"
  },
  "autoload": {
    "psr-4": {
      "Qualtrics\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Qualtrics\\Tests\\": "tests/src/"
    }
  }
}
